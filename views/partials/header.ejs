<script>
  // onclick agree
  function agree() {
    var agobj = document.getElementById("disclaimer");
    agobj.remove();
  }
    // onclick Disagree
  function disagree() {
    var x = document.getElementById("prev");
    var y = document.getElementById("disclaimer-content");
    if (x.style.display === "none") {
      x.style.display = "block";
      y.style.display = "none";
    }
  }
    // onclick back
  function back() {
    var a = document.getElementById("prev");
    var b = document.getElementById("disclaimer-content");
    if (b.style.display === "none") {
      b.style.display = "block";
      a.style.display = "none";
    }
  }
  // setCookie
  function setCookie(cname, cvalue) {
    var cname = "disclaimermsg";
    var cvalue = true;
    var date = new Date();
    date.setTime(date.getTime() + 24 * 60 * 60 * 1000);
    var expires = "expires=" + date.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
  }
</script>
